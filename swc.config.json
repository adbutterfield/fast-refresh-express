{
  "jsc": {
    "externalHelpers": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "keepClassNames": true,
    "experimental": {
      "plugins": [
        [
          "@swc/plugin-styled-components",
          {
            "displayName": false,
            "ssr": true
          }
        ],
        ["@swc/plugin-loadable-components", {}]
      ]
    }
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true
}
