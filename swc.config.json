{
  "jsc": {
    "externalHelpers": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "refresh": true
      }
    },
    "keepClassNames": true,
    "experimental": {
      "plugins": [
        [
          "@swc/plugin-styled-components",
          {
            "displayName": false,
            "ssr": true
          }
        ]
      ]
    }
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true
}
